<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.*?>


<VBox fx:id="rootVBox" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Boundary.userPanelRenter">
        <stylesheets>
    <URL value="@../style.css" />
  </stylesheets>
    <children>
    <MenuBar style="-fx-background-color: #343a40; -fx-text-fill: red;" VBox.vgrow="NEVER">
         <menus>
             <Menu mnemonicParsing="false" text="FERSA">
                 <items>
                     <MenuItem mnemonicParsing="false" text="Login" onAction="#login"/>
                     <MenuItem mnemonicParsing="false" text="Visualizza segnalazioni pagamento" onAction="#seePaymentClaim" />
                     <MenuItem mnemonicParsing="false" text="Inoltra segnalazione pagamento" onAction="#createPaymentClaim" />
                 </items></Menu>
         </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="seePaymentClaimButton" layoutX="134.0" layoutY="78.0" mnemonicParsing="false" prefHeight="57.0" prefWidth="389.0" onAction="#seePaymentClaim" text="Visualizza Segnalazioni Pagamento" />
            <Button fx:id="createPaymentClaimButton" layoutX="134.0" layoutY="173.0" mnemonicParsing="false" prefHeight="57.0" prefWidth="389.0" text="Inoltra Segnalazione Pagamento" onAction="#createPaymentClaim" />
            <Label fx:id="welcomeText" layoutX="134.0" layoutY="35.0" prefHeight="16.0" prefWidth="389.0" text="Bentornato &lt;nome_utente&gt;!" textAlignment="CENTER" wrapText="true" />
         </children>
    </AnchorPane>
  </children>
</VBox>